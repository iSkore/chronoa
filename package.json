        "build-docker": "vue-cli-service build && [ -d build ] || mkdir build && mv dist/* build/"
    },
    "dependencies": {
        "@mdi/font": "^5.9.55",
        "axios": "^0.21.1",
        "moment": "^2.29.1",
        "pino": "^6.13.1",
